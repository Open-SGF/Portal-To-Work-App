        "axios": "^0.19.2",
        "google-maps": "^4.3.2",
        "quasar": "1.12.12",
        "throttle-debounce": "^2.3.0",
        "timeago.js": "^4.0.2",
        "vue-analytics": "^5.22.1",
        "vue-gtm": "^3.5.0",
        "vue-instantsearch": "^3.1.0"
    },
    "devDependencies": {
        "@quasar/app": "^2.0.6",
        "@quasar/quasar-app-extension-dotenv": "^1.0.5"
    },
    "engines": {
        "node": ">= 8.9.0",
