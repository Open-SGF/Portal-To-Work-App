{
    "name": "portal-to-work",
    "version": "0.0.1",
    "description": "An app for finding jobs near you",
    "productName": "Portal to Work",
    "cordovaId": "com.mojobs.portaltowork",
    "capacitorId": "",
    "author": "Levi Zitting <me@levizitting.com>",
    "private": true,
    "scripts": {
        "dev": "quasar dev",
        "dev:android": "quasar dev --mode cordova --target android",
        "dev:ios": "quasar dev --mode cordova --target ios",
